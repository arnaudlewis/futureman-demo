- var whatWeDo = slice.value.toArray()[0]

section.content-3.studio
  .container
    .row
      .col-xs-10.col-sm-12.col-xs-offset-1.col-sm-offset-0
        svg.line(xmlns='http://www.w3.org/2000/svg', xmlns:xlink='http://www.w3.org/1999/xlink')
          line(x1='0', y1='5', x2='1000', y2='5')
        != whatWeDo.getStructuredText('title1').asHtml(ctx.linkResolver)
    .row
      .col-xs-10.col-sm-10.col-sm-offset-1.col-md-4.col-xs-offset-1.col-md-offset-0
        img(src=whatWeDo.getImage('leftImage').url, width='100', alt='Design')
        h4
          span.red +
          |  !{whatWeDo.getText('leftTitle')}
        != whatWeDo.getStructuredText('leftText').asHtml(ctx.linkResolver)
      .col-xs-10.col-sm-10.col-sm-offset-1.col-md-4.col-xs-offset-1.col-md-offset-0
        img(src=whatWeDo.getImage('centerImage').url, width='100', alt='Interactive')
        h4
          span.red +
          |  !{whatWeDo.getText('centerTitle')}
        != whatWeDo.getStructuredText('centerText').asHtml(ctx.linkResolver)
      .col-xs-10.col-sm-10.col-sm-offset-1.col-md-4.col-xs-offset-1.col-md-offset-0
        img(src=whatWeDo.getImage('rightImage').url, width='100', alt='Production')
        h4
          span.red +
          |  !{whatWeDo.getText('rightTitle')}
        != whatWeDo.getStructuredText('rightText').asHtml(ctx.linkResolver)
